{
  "mcpServers": {
    "mimo": {
      "type": "stdio",
      "command": "bun",
      "args": ["/root/mrc-server/mimo-mcp/bin/mimo-bun-wrapper.js"],
      "env": {
        "LC_ALL": "C.UTF-8",
        "LANG": "C.UTF-8",
        "ELIXIR_ERL_OPTIONS": "+fnu",
        "MIX_ENV": "dev",
        "MIMO_DIR": "/root/mrc-server/mimo-mcp",
        "PROMETHEUS_DISABLED": "true",
        "MIMO_DISABLE_HTTP": "true",
        "MIMO_WRAPPER_KILL_EXISTING": "0"
      },
      "disabled": false
    }
  }
}
