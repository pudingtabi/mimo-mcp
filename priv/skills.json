{
  "filesystem": {
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-filesystem", "/workspace"],
    "env": {}
  },
  "exa_search": {
    "command": "npx",
    "args": ["-y", "exa-mcp-server"],
    "env": {
      "EXA_API_KEY": "${EXA_API_KEY}"
    }
  },
  "fetch": {
    "command": "npx",
    "args": ["-y", "@tokenizin/mcp-npx-fetch"],
    "env": {}
  },
  "puppeteer": {
    "command": "docker",
    "args": ["run", "-i", "--rm", "--init", "--cap-add=SYS_ADMIN", "alpine-puppeteer-mcp"],
    "env": {}
  },
  "sequential_thinking": {
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
    "env": {}
  },
  "knowledge_graph": {
    "command": "npx",
    "args": ["-y", "mcp-knowledge-graph", "--memory-path", "/workspace/.aim/"],
    "env": {}
  },
  "sqlite": {
    "command": "npx",
    "args": ["-y", "mcp-server-sqlite", "--db", "/workspace/mimo.db"],
    "env": {}
  }
}
